<?xml version="1.0" encoding="UTF-8"?>
<simulation>
	<initParam>
		<simName>Wator</simName>
		<author>Callie</author>
		<xRows>30</xRows>
		<yCols>30</yCols>
		<fps>5</fps>
		<gridLines>0</gridLines>
	</initParam>
	<simParam>
	    <boundary>Finite</boundary>
	</simParam>
	<cellParam>    
	     <cell state="Cell" name="EmptyCell" color="Cyan" />  
	     <cell state="SharkCell" name="SharkCell" color="Black">
	        <locProbability>.2</locProbability>
	        <reproductionAge>3</reproductionAge>
			<maxEnergy>2</maxEnergy>
	     </cell>

	   	 <cell state="FishCell" name="FishCell" color="Yellow">
	        <locProbability>.3</locProbability>
	     	<reproductionAge>3</reproductionAge>

	    </cell>

  </cellParam>
</simulation>
	  
	    
 
